{
  "name": "angular-starter",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build:dev": "ng build && ng run angular-starter:server && npm run compile:server",
    "build": "ng build --prod",
    "build:client-and-server-bundles": "ng build --prod --configuration=production && ng run angular-starter:server:production",
    "build:prerender": "npm run build:client-and-server-bundles && npm run compile:server && npm run generate:prerender",
    "build:universal": "npm run build:client-and-server-bundles && npm run compile:server",
    "compile:server": "tsc -p tsconfig.server.json",
    "generate:prerender": "cd dist && node prerender",
    "serve:prerender": "cd dist && http-server",
    "serve:ssr": "node dist/server",
    "serve:ssr-dev": "nodemon --watch src/ --exec node dist/server --verbose  -e ts,html,scss",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/http": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/platform-server": "~7.2.0",
    "@angular/router": "~7.2.0",
    "@nguniversal/common": "7.1.1",
    "@nguniversal/express-engine": "7.1.1",
    "@nguniversal/module-map-ngfactory-loader": "7.1.1",
    "@ngx-translate/core": "11.0.1",
    "@ngx-translate/http-loader": "4.0.0",
    "@trademe/ng-defer-load": "3.0.0",
    "body-parser": "1.18.3",
    "classlist.js": "1.1.20150312",
    "cookie-parser": "1.4.3",
    "core-js": "^2.5.4",
    "csurf": "1.9.0",
    "express": "^4.15.2",
    "fs": "0.0.1-security",
    "jquery": "~3.4.0",
    "ng-lazyload-image": "5.1.2",
    "ngx-bootstrap": "^3.3.0",
    "ngx-cookie-service": "^2.2.0",
    "ngx-device-detector": "1.3.6",
    "ngx-owl-carousel": "2.0.7",
    "ngx-swiper-wrapper": "^8.0.1",
    "npm": "6.14.6",
    "path": "0.12.7",
    "popper.js": "1.14.4",
    "reflect-metadata": "^0.1.10",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26",
    "openid-client" : "1.2.0",
    "memory-cache" : "0.2.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.12.0",
    "@angular/cli": "~7.2.2",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "http-server": "^0.10.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-loader": "^4.2.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2"
  }
}
